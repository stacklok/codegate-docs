{
  "redirects": [
    {
      "source": "/how-to/(use-with-):path(aider|cline|continue|copilot)",
      "destination": "/codegate/integrations/:path",
      "permanent": true
    },
    {
      "source": "/features/secrets-encryption",
      "destination": "/codegate/features/secrets-redaction",
      "permanent": true
    },
    {
      "source": "/:path((?:about|development|features|how-to|integrations|partials|quickstart).*)*",
      "destination": "/codegate/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "codegate-docs-git-website-refactor-stacklok.vercel.app"
        }
      ]
    }
  ]
}
